#include<stdio.h>
int main(){
int n,a;
printf("No of elements in Array  ");
scanf("%d",&n);
int arr[n];
int count=0;
float avg=0;
for(int i=0;i<n;i++){
    printf("Enter the  element  ");
    scanf("%d",&a);
    arr[i]=a;
}
for(int i=0;i<n;i++){
    count+=arr[i];
}
avg=count/n;
printf("AVERAGE IS ");
printf("%f",avg);
return 0;
}
